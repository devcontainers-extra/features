{
    "id": "projen",
    "version": "1.0.2",
    "name": "projen (via npm)",
    "documentationURL": "http://github.com/devcontainers-extra/features/tree/main/src/projen",
    "description": "projen synthesizes project configuration files such as package.json, tsconfig.json, .gitignore, GitHub Workflows, eslint, jest, etc from a well-typed definition written in JavaScript.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-extra/features/npm-package",
        "ghcr.io/devcontainers/features/node"
    ]
}