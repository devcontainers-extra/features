{
    "id": "gh-cli",
    "version": "1.0.12",
    "name": "GitHub CLI (via GitHub Releases)",
    "documentationURL": "http://github.com/devcontainers-extra/features/tree/main/src/gh-cli",
    "description": "GitHub CLI is a command-line interface to GitHub for use in your terminal or your scripts.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-extra/features/gh-release"
    ]
}