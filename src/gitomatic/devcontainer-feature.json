{
    "id": "gitomatic",
    "version": "1.0.14",
    "name": "git-o-matic (gitomatic) (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-extra/features/tree/main/src/gitomatic",
    "description": "gitomatic is a tool to monitor git repositories and automatically pull & push changes.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-extra/features/gh-release"
    ]
}